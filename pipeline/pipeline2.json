{
	"name": "pipeline2",
	"properties": {
		"activities": [
			{
				"name": "Tevervroegenorders_log",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "FileServerWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "ACC_BYOD_DEV_DS",
						"type": "DatasetReference",
						"parameters": {
							"SchemaName": "dwh",
							"TableName": "vw_te_vervroegen_ordersV4"
						}
					}
				],
				"outputs": [
					{
						"referenceName": "tevervroegenACC_log",
						"type": "DatasetReference",
						"parameters": {
							"logfile": {
								"value": " @concat('log_', formatDateTime(utcnow(), 'yyyy-dd-MM'), '.csv')",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"annotations": []
	}
}