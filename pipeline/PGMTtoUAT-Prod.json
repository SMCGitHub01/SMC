				],
				"outputs": [
					{
						"referenceName": "DataverseExport_UAT",
						"type": "DatasetReference",
						"parameters": {
							"DataverseTable": "smc_smart_application_eng_tracking_list"
						}
					}
				]
			}
		],
		"folder": {
			"name": "PRD-TO-DATAVERSE"
