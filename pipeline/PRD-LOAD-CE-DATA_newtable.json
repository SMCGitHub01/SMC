{
	"name": "PRD-LOAD-CE-DATA_newtable",
	"properties": {
		"activities": [
			{
				"name": "GetNewTable",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "CommonDataServiceForAppsSource"
					},
					"sink": {
						"type": "AzureSqlSink",
						"preCopyScript": {
							"value": "DROP TABLE IF EXISTS ce.@variables('TableName')",
							"type": "Expression"
						},
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false,
						"tableOption": "autoCreate",
						"disableMetricsCollection": false
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "DataverseCE",
						"type": "DatasetReference",
						"parameters": {
							"table": {
								"value": "@variables('TableName')",
								"type": "Expression"
							}
						}
					}
				],
				"outputs": [
					{
						"referenceName": "PRD_BYOD_DS",
						"type": "DatasetReference",
						"parameters": {
							"SchemaName": "ce",
							"TableName": {
								"value": "@variables('TableName')",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"variables": {
			"TableName": {
				"type": "String"
			}
		},
		"folder": {
			"name": "PRD-CE"
		},
		"annotations": []
	}
}